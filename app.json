{
  "expo": {
    "name": "Murmur",
    "slug": "murmur",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "murmur",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f1419"
    },
    "ios": {
      "supportsTablet": true,
      "deploymentTarget": "18.0",
      "platformVersion": "18.0",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Murmur needs access to your microphone to record voice notes",
        "NSSpeechRecognitionUsageDescription": "Murmur uses speech recognition to transcribe your voice notes"
      },
      "bundleIdentifier": "com.casataqafa.murmur"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.casataqafa.murmur",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-voice/voice",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you attach images to notes.",
          "cameraPermission": "The app accesses your camera to let you take photos for notes."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fadc3aaf-c6c2-4352-bb0e-1d5dadabfcb2"
      }
    }
  }
}
